{
	"name": "@azuliani/transliterate",
	"version": "2.3.1",
	"description": "Convert Unicode characters to Latin characters using transliteration",
	"license": "MIT",
	"repository": "azuliani/transliterate",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"require": "./index.cjs",
			"import": "./index.js"
		}
	},
	"main": "./index.cjs",
	"sideEffects": false,
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "tsup",
		"prepare": "test -f dist/index.cjs || (test -d node_modules/tsup && npm run build) || true",
		"prepublishOnly": "npm run build",
		"test": "xo && ava"
	},
	"files": [
		"index.js",
		"index.cjs",
		"index.d.ts",
		"replacements.js",
		"locale-replacements.js",
		"tsup.config.js",
		"dist"
	],
	"keywords": [
		"transliterate",
		"transliteration",
		"string",
		"deburr",
		"unicode",
		"ascii",
		"text",
		"latin",
		"latinize",
		"convert",
		"replace"
	],
	"devDependencies": {
		"ava": "^6.4.1",
		"tsup": "^8.5.1",
		"xo": "^1.2.2"
	}
}
